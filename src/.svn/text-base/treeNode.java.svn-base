
public class treeNode {
	
	String name, v = new String("V");
	myNode displayNode;
	public treeNode(myNode a){
		displayNode = a;
		name = ""+displayNode.level+","+displayNode.position+" ("+v+")";
	}
	
	public String toString(){
		return name;
	}
	public void updateName(){
		name = ""+displayNode.level+","+displayNode.position+" ("+v+")";
	}
	public myNode getNode(){
		return displayNode;
	}
	public void setVisible(String a){
		v = a;
	}
	
}
